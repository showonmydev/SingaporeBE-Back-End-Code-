{
  "parser": "babel-eslint",
  "env": {
    "browser": true,
    "node": true,
    "mocha": true
  },
  "extends": "airbnb",
  "rules": {
    "func-names": 0,
    "max-len": 0,
    "no-unused-vars": 0,
    "no-script-url": 0,
    "import/no-unresolved": 0,
    "import/no-extraneous-dependencies": 0,
    "no-console": 0,
    "no-undef": 0,
    "import/prefer-default-export": 0,
    "global-require": 0,
    "comma-dangle": ["off"],
    "import/extensions": 0,
    "jsx-a11y/label-has-for": 0,
    "guard-for-in": ["warn"],
    "no-restricted-syntax": ["warn"],
    "object-curly-spacing": ["off"],
    "padded-blocks": ["off"],
    "no-underscore-dangle": [ "error", { "allowAfterThis": true }],
    "no-plusplus": 0,
    "no-shadow": 0,
    "class-methods-use-this": ["warn"]
  }
}
